{
  "name": "hosjoy-wi-ui",
  "version": "2.0.9",
  "private": false,
  "description": "一款基于taro的ui组件,比taroui更好用",
  "main": "dist/index.js",
  "templateInfo": {
    "name": "default",
    "typescript": false,
    "css": "sass"
  },
  "sideEffects": [
    "*.scss",
    "*.css"
  ],
  "keywords": [
    "wi",
    "wi-ui",
    "ui"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "build:component": "cross-env TARO_BUILD_TYPE=component taro build --ui",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "test": "cross-env NODE_ENV=test && jest --coverage"
  },
  "author": "hosjoy",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.6",
    "babel-runtime": "^6.26.0",
    "regenerator-runtime": "0.11.1",
    "nerv-devtools": "^1.5.5",
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "@tarojs/components": "2.1.5",
    "@tarojs/components-qa": "2.1.5",
    "@tarojs/router": "2.1.5",
    "@tarojs/taro": "2.1.5",
    "@tarojs/taro-alipay": "2.1.5",
    "@tarojs/taro-h5": "2.1.5",
    "@tarojs/taro-qq": "2.1.5",
    "@tarojs/taro-quickapp": "2.1.5",
    "@tarojs/taro-swan": "2.1.5",
    "@tarojs/taro-tt": "2.1.5",
    "@tarojs/taro-weapp": "2.1.5",
    "@tarojs/taro-rn": "2.1.5",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@tarojs/mini-runner": "2.1.5",
    "@tarojs/webpack-runner": "2.1.5",
    "@types/react": "^16.4.6",
    "@types/webpack-env": "^1.13.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^26.0.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-jsx-stylesheet": "^0.6.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^7.0.2",
    "eslint": "^5.16.0",
    "eslint-config-taro": "2.1.5",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-taro": "2.1.5",
    "jest": "^23.6.0",
    "nerv-server": "^1.5.1",
    "nerv-test-utils": "^1.5.1",
    "nervjs": "^1.5.6",
    "simulant": "^0.2.2",
    "stylelint": "9.3.0",
    "stylelint-config-taro-rn": "2.1.5",
    "stylelint-taro-rn": "2.1.5"
  }
}
